<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx" 
			   xmlns:local="*" 
			   minWidth="200" 
			   minHeight="180" initialize="{initApp()}">
	<fx:Declarations>
		<!-- 将非可视元素（例如服务、值对象）放在此处 -->

	
		
	</fx:Declarations>
	<fx:Script>
		<![CDATA[
			
			import flash.external.*;
			
			public function taction(type:String):void{
			    trace(type);
				if(type=='blink')
				   character.blink();
				else if(type=='left')
				   character.lookLeft();
				else
				   character.lookUp();
				
			}
			
		
			public function initApp():void {
				ExternalInterface.addCallback("testAction",taction);
			}
			
		]]>
	</fx:Script>
	
	<mx:VBox paddingLeft="50">
		<mx:HBox paddingTop="50">
			<local:Character id="character" x="100" y="100" width="150" height="120"/>
		</mx:HBox>
	</mx:VBox>
	
</s:Application>